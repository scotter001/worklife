package vn.worklife.user.quanlycau.model;

import org.apache.ibatis.type.Alias;

@Alias("TinhTrangCauVO")
public class TinhTrangCauVO {
    private int id;
    private int idCau;
    private int  nam;
    private int quy;
    private int tinhTrang;
    private String ghiChu;
    private String ngayBC;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public int getIdCau() {
        return idCau;
    }

    public void setIdCau(int idCau) {
        this.idCau = idCau;
    }

    public int getNam() {
        return nam;
    }

    public void setNam(int nam) {
        this.nam = nam;
    }

    public int getQuy() {
        return quy;
    }

    public void setQuy(int quy) {
        this.quy = quy;
    }

    public int getTinhTrang() {
        return tinhTrang;
    }

    public void setTinhTrang(int tinhTrang) {
        this.tinhTrang = tinhTrang;
    }

    public String getGhiChu() {
        return ghiChu;
    }

    public void setGhiChu(String ghiChu) {
        this.ghiChu = ghiChu;
    }

    public String getNgayBC() {
        return ngayBC;
    }

    public void setNgayBC(String ngayBC) {
        this.ngayBC = ngayBC;
    }
}
