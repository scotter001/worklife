package vn.worklife.user.util.model;

public class MappingFieldVO {
    private String data;
    private boolean isBold;
    private boolean isItalic;
    private boolean removeIfEmpty;
    private boolean haveNewLine;


    public MappingFieldVO(String data) {
        this.data = data;
        this.isBold = false;
    }
    public MappingFieldVO(String data, boolean isBold) {
        this.data = data;
        this.isBold = isBold;
    }
    public MappingFieldVO(String data, boolean isBold, boolean isItalic) {
        this.data = data;
        this.isBold = isBold;
        this.isItalic = isItalic;
    }

    public MappingFieldVO(String data, boolean isBold, boolean isItalic, boolean removeIfEmpty) {
        this.data = data;
        this.isBold = isBold;
        this.isItalic = isItalic;
        this.removeIfEmpty = removeIfEmpty;
    }

    public MappingFieldVO(String data, boolean isBold, boolean isItalic, boolean removeIfEmpty, boolean haveNewLine) {
        this.data = data;
        this.isBold = isBold;
        this.isItalic = isItalic;
        this.removeIfEmpty = removeIfEmpty;
        this.haveNewLine = haveNewLine;
    }

    public boolean isRemoveIfEmpty() {
        return removeIfEmpty;
    }

    public void setRemoveIfEmpty(boolean removeIfEmpty) {
        this.removeIfEmpty = removeIfEmpty;
    }

    public boolean isHaveNewLine() {
        return haveNewLine;
    }

    public void setHaveNewLine(boolean haveNewLine) {
        this.haveNewLine = haveNewLine;
    }

    public String getData() {
        return data;
    }

    public void setData(String data) {
        this.data = data;
    }

    public boolean isBold() {
        return isBold;
    }

    public void setBold(boolean bold) {
        isBold = bold;
    }

    public boolean isItalic() {
        return isItalic;
    }

    public void setItalic(boolean italic) {
        isItalic = italic;
    }
}
